QTabWidget::pane {
  border: 0;
}

QTabBar::tab {

  padding: 10px 12px;
  min-height: 34px;
  font-weight: 600;
}

QLabel[role="heading"] {
  font-size: 18px;
  font-weight: 600;
}

QFrame[role="card"] {
  border: 1px solid #b8b8b8;
  border-radius: 10px;
  background: #f7f7f7;
}

QFrame[role="sectionSeparator"] {
  color: #cfcfcf;
}
